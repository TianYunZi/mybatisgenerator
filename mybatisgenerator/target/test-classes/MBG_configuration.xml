<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!-- 去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 生发环境数据库配置:修改配置 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.18.158:3306/siebre-uaa?useSSL=false" userId="root" password="1qaz@WSX">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 持久化对象：targetPackage：修改包名 targetProject：修改项目名 或 去除 -->
		<javaModelGenerator targetPackage="com.siebre.uaa.organization.entity" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 数据访问对象接口: targetPackage：修改包名 targetProject：修改项目名 或 去除 -->
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 数据访问对象xml实现:targetPackage：修改包名 targetProject：修改项目名 或 去除 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.siebre.uaa.organization.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 表转代码：tableName：修改表名-->
		<table tableName="sys_organization" enableCountByExample="false" enableUpdateByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" enableInsert="true"
			   enableUpdateByPrimaryKey="true" enableDeleteByPrimaryKey="true" domainObjectName="Organization">
			   <columnOverride column="type" javaType="com.siebre.uaa.organization.OrganizationType" typeHandler="com.siebre.basic.enumutil.EnumStatusHandler" ></columnOverride>
			<!--<columnOverride column="account_type" javaType="com.siebre.payment.entity.enums.BankAccountType" typeHandler="com.siebre.basic.enumutil.EnumStatusHandler" ></columnOverride>-->
		</table>
	</context>
</generatorConfiguration>